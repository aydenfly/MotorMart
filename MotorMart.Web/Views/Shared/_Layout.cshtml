@model MotorMart.Web.Models.MasterViewModel

<!DOCTYPE html>
<html xmlns:fb="http://ogp.me/ns/fb#">
<head>
    <meta content="text/html; charset=utf-8" />
    <link rel="shortcut icon" href="/favicon.ico" type="image/x-icon" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <title>Motor Mart | Your Vehicle Wallstreet</title>
    <meta name="description" content="" />
    <meta name="keywords" content="" />
    
    <link href='http://fonts.googleapis.com/css?family=Signika:400,300,600,700|Signika+Negative:400,300,600,700&subset=latin,latin-ext' rel='stylesheet' type='text/css'>

    <link type="text/css" href="@Url.Content("~/Resources/css/jquery.qtip.css")" rel="stylesheet"  />

    <link type="text/css" href="@Url.Content("~/Resources/css/smoothness/jquery-ui-1.8.16.custom.css")" rel="stylesheet"  />
    <link type="text/css" href="@Url.Content("~/Resources/css/global.css")" rel="stylesheet"  />
    <link type="text/css" href="@Url.Content("~/Resources/css/motormart.css")" rel="stylesheet"  />
    <link href="@Url.Content("~/Resources/css/slimbox/slimbox2.css")" rel="stylesheet" type="text/css" />     
    
    <script src="@Url.Content("~/Resources/scripts/modernizr-1.7.min.js")" type="text/javascript"></script>   

    @RenderSection("HeadContent", false)    
</head>
<body>
    @* @Html.Debug(Model)*@
    <div class="page">
        <div id="header">
            <div id="logo">
                @Html.ActionLinkImageButton("home", "index", null, "/resources/images/logos/", "motormart_logo.png", "MotorMart | Vehicle wallstreet", null)
            </div>
            <div id="primary-nav">
                @{Html.RenderPartial("PrimaryMenu");}
            </div>
        </div>
        <div id="main">
            @RenderBody()
        </div>
        <div id="footer">
            @RenderSection("Footer", false)
        </div>
    </div>
    <text>
        <script type="text/javascript" src="@Url.Content("~/Resources/scripts/jquery-1.9.1.min.js")"></script>
        <script type="text/javascript" src="@Url.Content("~/Resources/scripts/jquery-ui-1.8.16.custom.min.js")"></script>
        <script type="text/javascript" src="@Url.Content("~/Resources/scripts/MicrosoftAjax.js")"></script>
        <script type="text/javascript" src="@Url.Content("~/Resources/scripts/MicrosoftMvcAjax.js")"></script>
        <script type="text/javascript" src="@Url.Content("~/Resources/scripts/MicrosoftMvcValidation.js")"></script>

        <script type="text/javascript" src="@Url.Content("~/Resources/scripts/slimbox2.js")"></script>
        <script type="text/javascript" src="@Url.Content("~/Resources/scripts/jquery.easing.1.3.js")"></script>
        <script type="text/javascript" src="@Url.Content("~/Resources/scripts/jquery.qtip.js")"></script>
        <script type="text/javascript" src="@Url.Content("/Resources/scripts/jquery.jqEasyCharCounter.js")"></script>
        <script type="text/javascript" src="@Url.Content("~/Resources/scripts/jq-common.js")"></script>
    </text> 
    @RenderSection("Scripts", false)
    <text>
        <script type="text/javascript" src="https://apis.google.com/js/plusone.js"></script>
        <script type="text/javascript">
            //FB Like button integration
            (function (d, s, id) {
                var js, fjs = d.getElementsByTagName(s)[0];
                if (d.getElementById(id)) return;
                js = d.createElement(s); js.id = id;
                js.src = "//connect.facebook.net/en_GB/all.js#xfbml=1&appId=157344931018704";
                fjs.parentNode.insertBefore(js, fjs);
            } (document, 'script', 'facebook-jssdk'));


            //Tweet button integration
            !function (d, s, id) {
                var js, fjs = d.getElementsByTagName(s)[0];
                if (!d.getElementById(id)) {
                    js = d.createElement(s);
                    js.id = id;
                    js.src = "https://platform.twitter.com/widgets.js";
                    fjs.parentNode.insertBefore(js, fjs);
                }
            } (document, "script", "twitter-wjs");

            //Google Plus integration
            window.___gcfg = {
                lang: 'en-US',
                parsetags: 'onload'
            };
            (function () {
                var po = document.createElement('script'); po.type = 'text/javascript'; po.async = true;
                po.src = 'https://apis.google.com/js/plusone.js';
                var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(po, s);
            })();

        </script>
    </text>
    <!--[if lt IE 9]>
        <script src="@Url.Content("~/Resources/scripts/IEHack.js")"></script>
    <![endif]-->
</body>
</html>
