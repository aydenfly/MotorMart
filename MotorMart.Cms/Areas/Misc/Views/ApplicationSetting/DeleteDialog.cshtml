@model MotorMart.Cms.Areas.Misc.Models.ApplicationSettingViewModel

@if (Model.delete.CurrentApplicationSetting != null)
{

<div id="dialog-delete" title="Delete '@Model.delete.CurrentApplicationSetting.name '?">
    @using (Html.BeginRouteForm("ApplicationSettingDelete", new { @applicationsettingid = Model.delete.applicationsettingid }, FormMethod.Post, new { }))
    {  
       @Html.AdminUIAlert("Warning", "You are about to delete '" + Model.delete.CurrentApplicationSetting.name + "'. This action can NOT be reversed.")       
        <div id="error-messages"></div>
        @Html.HiddenFor(model => model.delete.applicationsettingid)
    } 
</div> 
}
else
{

<div id="dialog-cancel" title="Unable to delete '@Model.delete.CurrentApplicationSetting.name '">    
    @Html.AdminUIAlert("Unable to delete '" + Model.delete.CurrentApplicationSetting.name + "'.")
</div> 
}
